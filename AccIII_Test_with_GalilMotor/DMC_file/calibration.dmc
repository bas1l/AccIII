#AUTO
READY=0;
REM ------------------------------
REM GENERAL SETTINGS
ER 1000;
OE 0,0;
MT 1,1;
CE 2,2;
CN -1,-1;
REM SET PID PARAMETERS
KP 5.0,5.0;
KI 0.0,0.0;
KD 0.0,0.0;
REM SERVO BOTH AXES
SH XY;
WT 250;
REM ------------------------------
REM LOWER TORQUE LIMIT
TL 0.5,0.5;
REM DISABLE OFF-ON-ERROR
OE 0,0;
REM ------------------------------
REM JOG THE X AXIS (4000)
JG -4000;
BG X;
REM JOG UNTIL ERROR EXCEEDS 500
#JOGX;
JP#JOGX,@ABS[_TEX]<500;
ST;
AM X;
REM ------------------------------
REM JOG THE Y AXIS (4000)
JG ,-4000;
BG Y;
REM JOG UNTIL ERROR EXCEEDS 500
#JOGY;
JP#JOGY,@ABS[_TEY]<500;
ST;
AM Y;
REM ------------------------------
REM RESET ZERO POSITION
DP 0,0
REM CENTER BOTH X AND Y AXIS
PR 20000,20000; '20000 before
BG XY;
AM XY;
REM ------------------------------
REM SET TORQUE LIMIT
TL 4,4
REM ENABLE OFF-ON-ERROR
OE 0,0
WT 100
REM DEFAULT SPEED AND ACCELERATION
REM TM 1000?
TM 250
SP 25000,25000
AC 256000,256000
DC 256000,256000
REM ------------------------------
READY=1;
EN